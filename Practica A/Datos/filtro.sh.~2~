# Script para filtrar renglones de un archivo que contengan las cadenas de caracteres dados

egrep -v 'PRES|hPa' allData | egrep '72274|CAPE|K|SWEAT|Richardson|Showalter|Totals' > "##DataFilter1.csv##"

echo "OK"
